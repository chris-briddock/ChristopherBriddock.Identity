{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Host={dbHost};Port={dbPort};Username={dbUsername};Password={dbPassword};Database={dbName};",
    "Redis": "{redisConnectionString}",
    "RedisInstanceName": "{redisInstanceName}"
  },
  "Jwt": {
    "Issuer": "{jwtIssuer}",
    "Audience": "{jwtAudience}",
    "Secret": "{jwtSecret}",
    "Expires": "{jwtExpires}"
  },
  "FeatureManagement": {
    "Redis": false,
    "ApplicationInsights": false,
    "Seq": false,
    "ServiceBus": false,
    "RabbitMq": false
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{appInsightInstrumentationKey}"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "{seqEndpointAddress}" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Messaging": {
    "RabbitMQ": {
      "Hostname": "{hostname}",
      "Username": "{rabbitUsername}",
      "Password": "{rabbitPassword}"
    },
    "AzureServiceBus": {
      "ConnectionString": "{connectionString}"
    }
  },
  "Seq": {
    "Endpoint": "{seqEndpointAddress}",
    "ApiKey": "{seqApiKey}"
  }
}